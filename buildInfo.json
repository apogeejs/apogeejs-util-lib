{
    "repoName": "apogeejs-util-lib",

    "esModule": {
        "taskList": [
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-util-lib/versionInfo.json"
                ]
            },
            {
                "type": "packageLib",
                "esModuleFileName": "apogeeUtilLib.js",
                "externalLibs": []
            }
        ]
    },
    "npmModule": {
        "taskList": [
            {
                "type": "copyAndReplace",
                "srcFileUrl": "/apogeejs-util-lib/package.json",
                "replacementList": [
                    {
                        "marker": "[RELEASE_VERSION]",
                        "name": "RELEASE_VERSION"
                    }
                ]
            },
            {
                "type": "packageLib",
                "esModuleFileName": "apogeeUtilLib.js",
                "npmModuleFileName": "apogeejs-util-lib.js"
            }
        ]
    }
}